{
	"name": "nextjs-hybrid-auth",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"format": "prettier --write .",
		"codegen": "openapi-ts",
		"test": "vitest",
		"test:watch": "vitest --watch",
		"test:ui": "vitest --ui",
		"coverage": "vitest run --coverage && node -e \"console.log('ðŸ“„ Coverage report: file:///' + require('path').resolve('coverage/index.html'))\"",
		"test:ci": "vitest run --coverage",
		"dev": "next dev",
		"build": "next build",
		"start": "docker-compose up --build",
		"lint": "next lint",
		"prepare": "npx husky",
		"start:backend-local": "docker-compose -f .devcontainer/docker-compose.prod.yml down -v && docker-compose -f .devcontainer/docker-compose.prod.yml up -d postgres redis smtp4dev && docker-compose -f .devcontainer/docker-compose.prod.yml run --rm migrate && docker-compose -f .devcontainer/docker-compose.prod.yml up --build -d --remove-orphans api",
		"generate:tokens": "tsx src/tokens/build.ts"
	},
	"dependencies": {
		"@edge-csrf/nextjs": "^2.5.3-cloudflare-rc1",
		"@fortawesome/free-solid-svg-icons": "^6.2.1",
		"@fortawesome/react-fontawesome": "^0.2.0",
		"@hey-api/client-fetch": "^0.13.0",
		"@hey-api/openapi-ts": "^0.72.0",
		"@mantine/core": "^8.1.0",
		"@mantine/form": "^8.1.0",
		"@mantine/hooks": "^8.1.0",
		"@mantine/notifications": "^8.1.0",
		"@mui/icons-material": "^5.8.4",
		"@tabler/icons-react": "^3.2.0",
		"@tanstack/react-query": "^5.80.6",
		"axios": "^1.9.0",
		"check-password-strength": "^2.0.10",
		"classnames": "^2.5.1",
		"date-fns": "^2.29.3",
		"next": "^15.3.3",
		"next-intl": "^4.1.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-icons": "^4.12.0",
		"react-scroll-wheel-handler": "^2.2.0",
		"react-toastify": "^10.0.6",
		"react-world-flags": "^1.6.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.8.1",
		"@commitlint/config-conventional": "^19.8.1",
		"@faker-js/faker": "^9.8.0",
		"@tanstack/react-query-devtools": "^5.80.6",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.3.0",
		"@testing-library/user-event": "^14.6.1",
		"@types/glob": "^8.1.0",
		"@types/jest": "^29.5.12",
		"@types/lodash": "^4.17.20",
		"@types/node": "^20.12.7",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"@types/react-world-flags": "^1.6.0",
		"@vitejs/plugin-react": "^4.7.0",
		"@vitest/coverage-v8": "^3.2.4",
		"@vitest/ui": "^3.2.4",
		"dotenv-cli": "^7.4.1",
		"eslint": "^9.34.0",
		"glob": "^11.0.3",
		"happy-dom": "^18.0.1",
		"jest": "^29.7.0",
		"jest-fixed-jsdom": "^0.0.2",
		"jest-html-reporter": "^3.10.2",
		"jest-junit": "^16.0.0",
		"jsdom": "^24.1.3",
		"msw": "^2.4.10",
		"next-router-mock": "^0.9.13",
		"postcss": "^8.4.38",
		"postcss-preset-mantine": "^1.14.4",
		"postcss-simple-vars": "^7.0.1",
		"prettier": "^3.6.2",
		"semantic-release": "^24.2.7",
		"sharp": "^0.33.3",
		"style-dictionary": "^5.0.2",
		"tsx": "^4.20.5",
		"typescript": "^5.4.5",
		"vitest": "^3.2.4"
	},
	"msw": {
		"workerDirectory": [
			"public"
		]
	}
}
